# =======================
# France Travail API (Offres d'emploi)
# Obtenez ces identifiants sur https://www.emploi-store-dev.fr/
# =======================
FRANCE_TRAVAIL_CLIENT_ID=your_client_id_here
FRANCE_TRAVAIL_CLIENT_SECRET=your_client_secret_here

# =======================
# API ROMEO (Entreprises)
# Obtenez ces identifiants sur le portail développeur France Travail
# =======================
ROMEO_CLIENT_ID=your_romeo_client_id_here
ROMEO_CLIENT_SECRET=your_romeo_client_secret_here

# =======================
# Configuration de la base de données
# =======================
DB_HOST=localhost
DB_PORT=5432
DB_NAME=job_search_app
DB_USER=postgres
DB_PASSWORD=your_secure_password_here

# =======================
# Paramètres optionnels
# =======================
# Taille du pool de connexions
DB_MIN_CONN=1
DB_MAX_CONN=10

# Niveau de log (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
